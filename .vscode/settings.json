{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.map": true,
        "*.js": true,
        "*.d.ts": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    },
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "cSpell.words": [
        "Astley",
        "NSURL",
        "Prebuffer",
        "Siri",
        "audioplayer",
        "exoplayer",
        "libvlc",
        "nospace",
        "prettierrc",
        "quotemark",
        "trackselection"
    ]
}